            layout="@layout/tittle_bottom_strip"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/card_background_image_andealr_fragment"
            />
        <include
            layout="@layout/likes_and_redeem"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/card_background_image_andealr_fragment"
            />
