    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">


        <ImageView
            android:id="@+id/card_background_image_andealr_fragment"
            android:layout_height="300dp"
            android:layout_width="405dp"
            android:scaleType="centerCrop"
            />
